{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/dist/assets/(.*)",
      "dest": "/dist/assets/$1"  
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ]
}